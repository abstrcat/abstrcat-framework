{
  "name": "abstrcat-core",
  "version": "0.1.1",
  "type": "module",
  "description": "The progressive JavaScript framework for building modern web UI.",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview"
  },
  "types": "dist/index.d.ts",
  "files": [
    "index.js",
    "index.umd.cjs",
    "dist"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "node": "./index.umd.cjs",
        "default": "./dist/index.umd.cjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "node": "./index.umd.cjs",
        "default": "./index.js"
      }
    },
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abstrcat/abstrcat-framework.git"
  },
  "keywords": [
    "abstrcat"
  ],
  "author": "Maksym Osovitnii",
  "license": "MIT",
  "homepage": "https://abstrcat.tech/",
  "devDependencies": {
    "abstrcat-core": "^0.1.1",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.2",
    "vite": "^6.0.5",
    "vite-plugin-dts": "^4.4.0"
  },
  "peerDependencies": {
    "typescript": "*"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
