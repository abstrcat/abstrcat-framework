{
  "name": "abstrcat-core",
  "version": "0.1.1",
  "type": "module",
  "description": "The progressive JavaScript framework for building modern web UI.",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "copyfiles -f ./lib/css/* ./dist/css && rimraf ./dist/css/output.css",
    "build:css": "npx tailwindcss -i ./lib/css/main.css -o ./public/css/output.css --watch"
  },
  "types": "dist/index.d.ts",
  "files": [
    "index.js",
    "index.umd.cjs",
    "dist",
    "css"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "node": "./index.umd.cjs",
        "default": "./dist/index.umd.cjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "node": "./index.umd.cjs",
        "default": "./index.js"
      }
    },
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abstrcat/abstrcat-framework.git"
  },
  "keywords": [
    "abstrcat"
  ],
  "author": "Maksym Osovitnii",
  "license": "MIT",
  "homepage": "https://abstrcat.tech/",
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "abstrcat-core": "^0.1.1",
    "autoprefixer": "^10.4.20",
    "copyfiles": "^2.4.1",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "rimraf": "^6.0.1",
    "tailwind-children": "^0.5.0",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.6.2",
    "vite": "^6.0.5",
    "vite-plugin-dts": "^4.4.0"
  },
  "peerDependencies": {
    "typescript": "*"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  }
}
